# Colorful less.
less='less -r'

ssh_dangerous='ssh -o UserKnownHostsFile=/dev/null -o "StrictHostKeyChecking no"'
sshr='ssh_dangerous -l root'
omz='cd ~/.oh-my-zsh'
editaliases='$EDITOR $ALIAS_FILE'
ga='git add'
gd='git diff'
grc='git rebase --continue'
grim='git rebase --interactive origin/master'
gs='git status'
history='fc -l 1'
rezsh='source ~/.zshrc'
rmpyc='rm **/*.pyc'
wip='git commit -am wip'
pullr='hub push dave $(git here) && hub pull-request'

# MFP
salt-root='cd ~/code/mfp-salt-vagrant/salt/roots'
pillar='cd ~/code/mfp-salt-vagrant/salt/pillar'
arc='$HOME/code/arcanist/bin/arc'

# SP

cdtf='cd ~/code/sp/terraform-config'
tfp='terraform plan -module-depth=1'
tfa='terraform apply'
